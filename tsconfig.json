{
  "compilerOptions": {
    "target": "ES2015",
    "lib": [
      "DOM",
      "ESNext"
    ],
    "removeComments": true,
    "module": "ESNext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "noEmit": true,
    "jsx": "react-jsx"
  },
  "include": [
    "src"
  ],
  "exclude": [
    "node_modules"
  ]
}